{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "interface-name":  true,
    "member-access": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "no-console": false,
    "arrow-parens": false,
    "no-empty": false
  }
}
